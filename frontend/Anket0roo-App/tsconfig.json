{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "rootDir": ".",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
  },
  "include": [
    "./src/**/**/\*"
  ]
}